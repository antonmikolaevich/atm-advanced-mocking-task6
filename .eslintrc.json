{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-undef": "warn",
        "no-unused-vars": "warn",
        "no-useless-escape": "off",
        "no-useless-catch": "off",
        "no-mixed-spaces-and-tabs": "off",
        "no-func-assign": "warn",
        "no-unsafe-finally": "off",
        "no-cond-assign": "off",
        "no-prototype-builtins": "off",
        "getter-return": "off",
        "no-empty": "off",
        "no-fallthrough": "off",
        "no-misleading-character-class": "off",
        "valid-typeof": "off"
    }
}
